#include <iostream>
using namespace std;
class Queue {
    int front, rear;
    int q[5];
public:
    Queue() {
        front = rear = -1;
    }
    void insert(int x) {
        if (rear == 4)
            cout << "Queue Full\n";
        else {
            if (front == -1) front = 0;
            q[++rear] = x;
        }
    }
    void remove() {
        if (front == -1 || front > rear)
            cout << "Queue Empty\n";
        else
            front++;
    }
    void display() {
        for (int i = front; i <= rear; i++)
            cout << q[i] << " ";
    }
};

int main() {
    Queue q;
    q.insert(1);
    q.insert(2);
    q.insert(3);
    q.display();
    return 0;
}
