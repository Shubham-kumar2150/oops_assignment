#include <iostream>
using namespace std;
class Stack {
    int top;
    int arr[5];
public:
    Stack() {
        top = -1;
    }
    void push(int x) {
        if (top == 4)
            cout << "Stack Overflow\n";
        else
            arr[++top] = x;
    }
    void pop() {
        if (top == -1)
            cout << "Stack Underflow\n";
        else
            top--;
    }
    void display() {
        for (int i = top; i >= 0; i--)
            cout << arr[i] << " ";
    }
};

int main() {
    Stack s;
    s.push(10);
    s.push(20);
    s.push(30);
    s.display();
    return 0;
}
