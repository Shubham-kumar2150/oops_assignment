#include <iostream>
using namespace std;
class B;
class A {
    int x;
public:
    A(int a) {
        x = a;
    }
    friend void swap(A&, B&);
    void show() {
        cout << "x = " << x << endl;
    }
};
class B {
    int y;
public:
    B(int b) {
        y = b;
    }
    friend void swap(A&, B&);
    void show() {
        cout << "y = " << y << endl;
    }
};

void swap(A &a, B &b) {
    int temp = a.x;
    a.x = b.y;
    b.y = temp;
}

int main() {
    A a(10);
    B b(20);
    cout << "Before swap:" << endl;
    a.show();
    b.show();
    swap(a, b);
    cout << "After swap:" << endl;
    a.show();
    b.show();

    return 0;
}
