#include <iostream>
using namespace std;
class Calc {
public:
    int a, b;
    void input() {
        cin >> a >> b;
    }
    void operations() {
        cout << "Add: " << a+b << endl;
        cout << "Sub: " << a-b << endl;
        cout << "Mul: " << a*b << endl;
        cout << "Div: " << a/b << endl;
    }
};
int main() {
    Calc c;
    c.input();
    c.operations();
    return 0;
}
