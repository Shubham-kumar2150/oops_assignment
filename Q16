#include <iostream>
using namespace std;
int main() {
    int a = 10;
    cout << "Pre-increment (++a): " << ++a << endl;
    cout << "Post-increment (a++): " << a++ << endl;
    cout << "Value of a after post-increment: " << a << 
    cout << "Pre-decrement (--a): " << --a << endl;
    cout << "Post-decrement (a--): " << a-- << endl;
    cout << "Value of a after post-decrement: " << a << endl;
    return 0;
}
