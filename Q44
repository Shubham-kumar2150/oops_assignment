#include <iostream>
using namespace std;
class Employee {
public:
    int id;
    string name;
    float salary;

    void input() {
        cin >> id >> name >> salary;
    }
    void display() {
        cout << id << " " << name << " " << salary;
    }
};
int main() {
    Employee e;
    e.input();
    e.display();
    return 0;
}
