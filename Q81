#include <iostream>
using namespace std;
class A {
public:
    void showA() {
        cout << "Class A (Base Class)" << endl;
    }
};
class B : public A {
public:
    void showB() {
        cout << "Class B (Derived Class)" << endl;
    }
};
int main() {
    B obj;
    obj.showA();
    obj.showB();
    return 0;
}
