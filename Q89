#include <iostream>
using namespace std;
class A {
public:
    void show() {
        cout << "Class A show()" << endl;
    }
};
class B {
public:
    void show() {
        cout << "Class B show()" << endl;
    }
};
class C : public A, public B {
};
int main() {
    C obj;
    // obj.show();  // Ambiguity Error
    return 0;
}
