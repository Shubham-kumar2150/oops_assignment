#include <iostream>
using namespace std;
int main() {
    int a = 10;
    int *p;
    p = &a;
    cout << "Value of a: " << a << endl;
    cout << "Address of a: " << &a << endl;
    cout << "Value stored in pointer p: " << p << endl;
    cout << "Value at pointer p: " << *p << endl;
    return 0;
}
