#include <iostream>
using namespace std;
int main() {
    int a = 10, b = 5, c = 0;
    cout << "a = " << a << ", b = " << b << ", c = " << c << endl << endl;
    cout << "(a > b && b > c) : " << (a > b && b > c) << endl
    cout << "(a < b || b > c) : " << (a < b || b > c) << endl;
    cout << "!(a > b) : " << !(a > b) << endl;
    return 0;
}
