#include <iostream>
using namespace std;
class Demo {
    int x;
public:
    void set(int x) {
        this->x = x;   // distinguishes data member from parameter
    }
    void show() {
        cout << "Value of x: " << x << endl;
    }
};
int main() {
    Demo d;
    d.set(10);
    d.show();
    return 0;
}
